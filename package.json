    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/uuid": "^9.0.0",
    "aws-cdk": "2.61.1",
    "esbuild": "^0.17.4",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.259.0",
    "@aws-sdk/client-s3": "^3.259.0",
    "@aws-sdk/util-dynamodb": "^3.262.0",
    "aws-cdk-lib": "2.61.1",
