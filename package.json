{
  "name": "app",
  "version": "0.1.0",
  "bin": {
    "app": "bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "check-types": "npx tsc --noEmit",
    "watch": "npx ts-node -r tsconfig-paths/register --swc ./nodemon.config.ts",
    "app": "npx ts-node -r tsconfig-paths/register --swc ./bin/index.ts",
    "dev": "npx concurrently --kill-others \"npm run app\" \"npm run watch\"",
    "start": "npm run check-types && npm run app",
    "deploy": "npx cdk deploy '**' --require-approval never",
    "secrets": "doppler secrets download --no-file --format env > .env"
  },
  "devDependencies": {
    "@octokit/webhooks-schemas": "=6.10.0",
    "@octokit/webhooks-types": "=6.10.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@swc/helpers": "^0.5.15",
    "@types/aws-lambda": "=8.10.110",
    "@types/html-to-text": "=9.0.0",
    "@types/jest": "=29.2.5",
    "@types/jsonwebtoken": "=9.0.1",
    "@types/lodash": "=4.14.191",
    "@types/node": "^20.17.24",
    "@types/request": "=2.48.8",
    "@types/uuid": "=9.0.0",
    "aws-cdk": "=2.115.0",
    "esbuild": "=0.17.4",
    "jest": "=29.3.1",
    "ts-jest": "=29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "=4.9.4"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.10.18"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "=3.259.0",
    "@aws-sdk/client-s3": "=3.259.0",
    "@aws-sdk/client-sfn": "=3.301.0",
    "@aws-sdk/util-dynamodb": "=3.262.0",
    "aws-cdk-lib": "=2.115.0",
    "aws-sdk": "=2.1303.0",
    "axios": "=1.7.9",
    "concurrently": "^9.1.2",
    "constructs": "=10.0.0",
    "date-fns": "=2.29.3",
    "dotenv": "=16.0.3",
    "email-validator": "=2.0.4",
    "googleapis": "=111.0.0",
    "html-to-text": "=9.0.4",
    "jimp": "=0.22.7",
    "jsonwebtoken": "=9.0.0",
    "libphonenumber-js": "=1.10.26",
    "lodash": "=4.17.21",
    "nodemon": "^3.1.9",
    "source-map-support": "=0.5.21",
    "tsconfig-paths": "^4.2.0",
    "uuid": "=9.0.0",
    "zod": "^3.23.8"
  }
}
