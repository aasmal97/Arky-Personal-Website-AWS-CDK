    "@aws-sdk/client-sfn": "=3.301.0",
    "@aws-sdk/util-dynamodb": "=3.262.0",
    "aws-cdk-lib": "=2.74.0",
    "aws-sdk": "=2.1303.0",
    "axios": "=1.3.2",
    "constructs": "=10.0.0",
    "date-fns": "=2.29.3",
    "dotenv": "=16.0.3",
    "googleapis": "=111.0.0",
    "html-to-text": "=9.0.4",
    "jimp": "=0.22.7",
    "jsonwebtoken": "=9.0.0",
    "lodash": "=4.17.21",
    "source-map-support": "=0.5.21",
    "uuid": "=9.0.0"
  }
