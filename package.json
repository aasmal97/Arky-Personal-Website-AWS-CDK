{
  "name": "app",
  "version": "0.1.0",
  "bin": {
    "app": "bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "check-types": "npx tsc --noEmit",
    "start:app": "npx ts-node -r tsconfig-paths/register --swc ./bin/index.ts",
    "watch": "npx ts-node -r tsconfig-paths/register --swc ./nodemon.config.ts",
    "dev": "docker compose up --build --watch",
    "synth": "npx cdk synth",
    "deploy": "npx cdk bootstrap && npx cdk deploy '**' --require-approval never",
    "secrets": "doppler secrets download --no-file --format env > .env"
  },
  "devDependencies": {
    "@octokit/webhooks-schemas": "=6.10.0",
    "@octokit/webhooks-types": "=6.10.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@swc/helpers": "^0.5.15",
    "@types/aws-lambda": "^8.10.110",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "=9.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.24",
    "@types/request": "^2.48.12",
    "@types/uuid": "^10.0.0",
    "aws-cdk": "~2.1003.0",
    "esbuild": "~0.21",
    "jest": "^29.7.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.2"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.10.18"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "=3.259.0",
    "@aws-sdk/client-s3": "=3.259.0",
    "@aws-sdk/client-sfn": "=3.301.0",
    "@aws-sdk/util-dynamodb": "=3.262.0",
    "aws-cdk-lib": "=2.182.0",
    "aws-sdk": "=2.1303.0",
    "axios": "=1.7.9",
    "concurrently": "^9.1.2",
    "constructs": "=10.0.0",
    "date-fns": "=2.29.3",
    "dotenv": "=16.0.3",
    "email-validator": "=2.0.4",
    "googleapis": "=111.0.0",
    "html-to-text": "^9.0.5",
    "jimp": "=0.22.7",
    "jsonwebtoken": "=9.0.0",
    "libphonenumber-js": "=1.10.26",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.9",
    "source-map-support": "=0.5.21",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  }
