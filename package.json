{
  "name": "app",
  "version": "0.1.0",
  "bin": {
    "app": "app/bin/app.js"
  },
  "scripts": {
    "build": "tsc -noEmit",
    "watch": "tsc -w",
    "test": "jest",
    "serve": "npm run secrets && npm run bootstrap && npm run deploy",
    "bootstrap": "npx ts-node app/buildScript.ts",
    "deploy": "cd ./app && cdk deploy '**' --require-approval never",
    "secrets": "doppler secrets download --no-file --format env > .env"
  },
  "devDependencies": {
    "@octokit/webhooks-schemas": "^6.10.0",
    "@octokit/webhooks-types": "^6.10.0",
    "@types/aws-lambda": "^8.10.110",
    "@types/babel__core": "^7.20.0",
    "@types/html-to-text": "^9.0.0",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.18",
    "@types/request": "^2.48.8",
    "@types/uuid": "^9.0.0",
    "aws-cdk": "^2.61.1",
    "esbuild": "^0.17.4",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "=2.74.0-alpha.0",
    "@aws-sdk/client-dynamodb": "=3.259.0",
    "@aws-sdk/client-s3": "=3.259.0",
